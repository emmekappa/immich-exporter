{
  "name": "immich-exporter",
  "version": "1.0.0",
  "description": "Immich Exporter for Prometheus",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsx --env-file=.env src/index.ts",
    "start": "node --env-file=.env dist/index.js",
    "docker:build": "docker build -t immich-exporter .",
    "docker:run": "docker run -ti -p 3000:3000 --env-file .env immich-exporter"
  },
  "keywords": [],
  "author": "emmekappa <emmekappa@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.1",
    "prom-client": "^15.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsx": "^4.20.3",
    "@types/node": "^24.0.15",
    "typescript": "^5.8.3",
    "@types/express": "^4.17.21"
  }
}
